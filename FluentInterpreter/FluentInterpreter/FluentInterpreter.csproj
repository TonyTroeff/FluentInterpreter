<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <Title>FluentInterpreter</Title>
        <Authors>TonyTroeff</Authors>
        <Description>NuGet package that adds more configuration features to the FluentAPI.</Description>
        <RepositoryUrl>https://bitbucket.org/TonyTroeff/fluentinterpreter</RepositoryUrl>
        <PackageVersion>1.0.0</PackageVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Humanizer.Core" Version="2.6.2" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.2.4" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.2.4" />
    </ItemGroup>
</Project>